    <h2>ジャンル編集</h2>
    <%= form_with model:@genre, url: admin_genre_path(@genre.id), local:true, method: :patch do |f| %>
      <div class="form-group">
        <%= f.label " ジャンル名" %>
        <%= f.text_field :name %>
      <div class="form-group">
        <%= f.submit "変更を保存", class: 'btn btn-secondary' %>
      </div>
    <% end %>
